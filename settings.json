{
    "workbench.colorTheme": "Activate SCARLET protocol (beta)",
    "editor.minimap.enabled": false,
    "files.autoSave": "onFocusChange",
    "editor.fontSize": 16,
    "editor.wordWrap": "wordWrapColumn",
    "files.autoGuessEncoding": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "breadcrumbs.enabled": true,
    "workbench.editor.highlightModifiedTabs": true,
    "search.smartCase": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "editor.stablePeek": true,
    "editor.formatOnSave": true,
    "explorer.confirmDelete": false,
    "editor.suggestSelection": "first",
    "window.zoomLevel": 1,
    "git.autofetch": true,
    "workbench.colorCustomizations": {
        "[Activate SCARLET protocol (beta)]": {
            "breadcrumb.background": "#290011",
            "breadcrumb.foreground": "#aa0202",
            "breadcrumbPicker.background": "#180000",
            "breadcrumb.activeSelectionForeground": "#91bd00",
            "breadcrumb.focusForeground": "#e5ff00",
            "terminal.background": "#0c1b13",
            "terminal.border": "#44FF8C"
        }
    },
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "material-icon-theme",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.CustomBrowser": "chrome",
    "terminal.integrated.fontFamily": "monospace",
    "material-icon-theme.activeIconPack": "react_redux"
}

